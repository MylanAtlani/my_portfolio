{
  "functions": {
    "app/api/contact/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/contact",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 